{
    "name": "img-style",
    "displayName": "img-style",
    "description": "insert css with width height url by cal images size",
    "version": "0.0.1",
    "publisher": "linzhi19",
    "baseImagesUrl": "preview.gif",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "repository":"https://github.com/hnlinzhi19/css-insert-img",
    "activationEvents": [
        "onCommand:extension.imgInsert"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.imgInsert",
                "title": "image insert"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "imgstyle configuration",
            "properties": {
                "imgstyle.tpl": {
                    "type": "string",
                    "default": "test",
                    "description": "imgstyle path settings"
                },
                "imgstyle.path": {
                    "type": "Array",
                    "default": [
                        "src/**/*.{png,jpg,gif,webp}"
                    ],
                    "description": "imgstyle path settings"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.17"
    },
    "dependencies": {
        "@types/lodash": "4.14.108",
        "arch": "2.1.0",
        "clipboardy": "1.2.3",
        "cross-spawn": "5.1.0",
        "execa": "0.8.0",
        "get-stream": "3.0.0",
        "globby": "^8.0.1",
        "is-stream": "1.1.0",
        "isexe": "2.0.0",
        "jimp": "^0.2.28",
        "lodash": "^4.17.10",
        "lru-cache": "4.1.3",
        "npm-run-path": "2.0.2",
        "p-finally": "1.0.0",
        "path-key": "2.0.1",
        "pseudomap": "1.0.2",
        "shebang-command": "1.2.0",
        "shebang-regex": "1.0.0",
        "signal-exit": "3.0.2",
        "strip-eof": "1.0.0",
        "which": "1.3.0",
        "yallist": "2.1.2"
    }
}
